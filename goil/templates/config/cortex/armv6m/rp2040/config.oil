    COMMONFLAGS = "-DPICO_BUILD=1";
    COMMONFLAGS = "-DPICO_CMAKE_BUILD_TYPE=\"Release\"";
    COMMONFLAGS = "-DPICO_COPY_TO_RAM=0";
