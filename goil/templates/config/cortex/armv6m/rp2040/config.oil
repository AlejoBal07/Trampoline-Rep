    COMMONFLAGS = "-DPICO_USE_BLOCKED_RAM=0";
  };
  
  INTERRUPT_COUNT nb_it {
    IT_TABLE_SIZE = 48;
  };
  
  /*
    * systick is an option on Cortex-M0+,
    * it is defined at the processor specific level.
    */
  INTERRUPT SysTick {
    VECT = 15;
    VECTOR_TYPE = HANDLER {
      NAME = "SysTick_Handler";
    };
    ACK = FALSE;
    SETPRIO = TRUE { NUMBER = -1; };
  };
  
