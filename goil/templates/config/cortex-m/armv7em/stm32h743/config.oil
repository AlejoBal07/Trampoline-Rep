  PLATFORM_FILES stm32h743 {
    PATH = "cortex-m/armv7em/stm32h743";
    CFILE = "handlers_stm32l432.c";
    CFILE = "startup_stm32l432.c";
    CFILE = "system_stm32l4xx.c";
    CFILE = "tpl_machine_stm32h743.c";
    CFILE = "tpl_memory_protection.c";
  };

  PLATFORM_FILES stm32h743_trace {
    PATH = "cortex-m/armv7em/stm32f303";
	  CFILE = "tpl_trace.c";
  };

  PLATFORM_FILES stm32h743_cmsis {
    PATH = "cortex-m/armv7em/stm32h743/CMSIS/Device/ST/STM32H7xx/Include";
  };

  LIBRARY serial {
    PATH = "cortex-m/armv7em/stm32l432/lib";
	  CHEADER = "serial.h";
	  CFILE   = "serial.c";
  };

  POSTBUILD all {
    COMMAND buildbin {
      TYPE = COPIER;
      INPUT = TARGET;
      OUTPUT = ".bin";
      PREOPTION = "-O binary";
    };
  };

  POSTCOMMAND burn {
    COMMAND flash {
      MESSAGE = "Flashing";
      COMMAND = "st-flash";
      INPUT = TARGET { EXT = ".bin"; };
      PREOPTION = "write";
      POSTOPTION = "0x8000000";
    };
  };
};
