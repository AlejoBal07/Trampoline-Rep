syntax goil_obj_appmode ("goil_lexique.gLexique"):

import "goil_options.gOption" ;
import "goil_semantics.gSemantics" ;

nonterminal <appmode> 
;

#
# external rules 
#
nonterminal <description>
    !@lstring desc  
;
nonterminal <free_field>
    ?!@ident_map ids  
;

rule <appmode> :
    @ident_map others [emptyMap] ;

    $APPMODE$ ;
    $idf$ ?* ;
    ${$ ;
    repeat
    while
        <free_field> !?others ; <description> ?* ; $;$ ;
    end repeat ;
    $}$ ;
    <description> ?* ;
    $;$ ;
end rule ;

end syntax ;
