semantics template_routines :
import semantics goil_template_invocation in "goil_template_invocation.ggs";

function goil_template
  ??@string prefix
  ??@goilTemplateVariableMap cfg
  ??@string name
  ->@string result
:
  result := "";
  goilTemplateInvocation
    ![@lstring new !name !here]
    !prefix
    !"templates"
    !emptySystemConfig[]
    !cfg
    !?result;
end function;
end semantics;