semantics templates_routines :
import semantics goil_template_invocation in "goil_template_invocation.ggs";

function goil_template
  ??@string prefix
  ??@string name
  ->@string result
:
  @systemConfig cfg := [@systemConfig new !"tadam !"];
  result := "";
  goilTemplateInvocation
    ![@lstring new !name !here]
    !prefix
    !"templates"
    !cfg
    ![@goilTemplateVariableMap emptyMap]
    !?result;
end function;
end semantics;