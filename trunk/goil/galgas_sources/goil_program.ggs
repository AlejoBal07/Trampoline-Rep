program goil_program "version 1.2.1" :
  import grammar goil_grammar in "goil_grammar.ggs" ;

#--- Prologue routine
  routine {
  }

#---
  when . "oil" message "an '.oil' source file" ??@lstring inSourceFile {
    grammar goil_grammar in inSourceFile ;
  }

  when . "OIL" message "an '.OIL' source file" ??@lstring inSourceFile {
    grammar goil_grammar in inSourceFile ;
  }

#--- Epilogue routine
  routine {
  }

end program ;
