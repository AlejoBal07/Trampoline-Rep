syntax template_parser ("template_scanner.gLexique") :

import "template_invocation.gSemantics";

nonterminal <goil_template_start_symbol>
  ??@string inPrefix
  ??@string inPath
  ??@string inTemplateDirectory
  ?@TfieldMap inTemplateVariableMap
  ?!@string ioResultingString
  !@Tvalue returnedValue
  !@Ttype returnedType
;

nonterminal <template_instruction>
  ??@string inPrefix
  ??@string inPath
  ??@string inTemplateDirectory
  ?!@TfieldMap ioTemplateVariableMap
  ?!@string ioResultingString
  !@Tvalue returnedValue
  !@Ttype returnedType
;

nonterminal <sorting_order>
  !@lsint order
;

#nonterminal <struct_path>
#  ?!@lstringlist path
##;

nonterminal <variable>
  ??@string inPrefix
  ??@string inPath
  ??@string inTemplateDirectory
  ??@TfieldMap inVariableMap
  !@TvarPath path
;

nonterminal <variable_or_here>
  ??@string inPrefix
  ??@string inPath
  ??@string inTemplateDirectory
  ??@TfieldMap inTemplateVariableMap
  !@location outWhere
;

nonterminal <expression>
  ??@string inPrefix
  ??@string inPath
  ??@string inTemplateDirectory
  ??@TfieldMap inTemplateVariableMap
  !@Tvalue outExpression
  !@Ttype outResultType
;

nonterminal  <template_instruction_list>
  ??@string inPrefix
  ??@string inPath
  ??@string inTemplateDirectory
  ?!@TfieldMap inTemplateVariableMap
  ?!@string ioResultingString
  !@Tvalue returnedValue
  !@Ttype returnedType
;

#---------------------------------------------------------------------------*

rule <template_instruction>
  ??@string inPrefix
  ??@string inPath
  ??@string inTemplateDirectory
  ?!@TfieldMap ioTemplateVariableMap
  ?!@string ioResultingString
  !@Tvalue returnedValue
  !@Ttype returnedType
:
  $!$;
  @Tvalue expression;
  @Ttype resultType;
  <expression>
    !inPrefix
    !inPath
    !inTemplateDirectory
    !ioTemplateVariableMap
    ?expression
    ?resultType
  ;
  switch resultType 
  when stringType, enumType:
    ioResultingString .= [expression mStringValue];
  when intType:
    ioResultingString .= [[expression mInt64Value] string];
  when floatType:
    ioResultingString .= [[expression mFloatValue] string];
  when boolType:
    if [expression mInt64Value] == 0LS then
      ioResultingString .= "false";
    else
      ioResultingString .= "true";
    end if;
  when listType :
    error here : "a list expression cannot be emitted";
  when mapType :
    error here : "a map expression cannot be emitted";
  when structType :
    error here : "a struct expression cannot be emitted";
  when unconstructedType, functionType:
    error here: "internal error, unconstructed type found";
  end switch; 
  
  returnedValue := valueWithVoid[];   
  returnedType := [@Ttype unconstructedType];   
end rule;

#---------------------------------------------------------------------------*

rule <template_instruction>
  ??@string inPrefix
  ??@string inPath
  ??@string inTemplateDirectory
  ?!@TfieldMap ioTemplateVariableMap
  ?!@string unused ioResultingString
  !@Tvalue returnedValue
  !@Ttype returnedType
:
  @bool fileNameOk := true;
  $write$; $to$;
  @Tvalue expression;
  @Ttype expressionType;
  @TfieldMap templateVariableMap := ioTemplateVariableMap;
  <expression>
    !inPrefix
    !inPath
    !inTemplateDirectory
    !ioTemplateVariableMap
    ?expression
    ?expressionType
  ;
#  message "EXPRESSION '" . [expression mStringValue] . "'\n" ;
  if [templateVariableMap hasKey !"FILENAME"] then
    [!?templateVariableMap removeKey !lstringWith[!"FILENAME"] ?* ?*];
  end if;
  if [templateVariableMap hasKey !"FILEPATH"] then
    [!?templateVariableMap removeKey !lstringWith[!"FILEPATH"] ?* ?*];
  end if;
  @Tvalue fileName := valueWithString[![[expression mStringValue] lastPathComponent]];
  @Tvalue nativeFilePath := valueWithString[![[expression mStringValue] nativePathWithUnixPath]];
  [!?templateVariableMap insertKey !lstringWith[!"FILENAME"] !expressionType !fileName];
  [!?templateVariableMap insertKey !lstringWith[!"FILEPATH"] !expressionType !nativeFilePath];
  @location fileNameLocation := here;
  if expressionType != [@Ttype stringType] then
    error here: "a file name should be of type string";
    fileNameOk := false;    
  end if;
  $:$;
  @string writeBlockResult := "";
  <template_instruction_list>
    !inPrefix
    !inPath
    !inTemplateDirectory
    !?templateVariableMap
    !?writeBlockResult
    ?*
    ?*
  ;
  if fileNameOk & [@uint errorCount] == 0 then
    [writeBlockResult makeDirectoryAndWriteToFile ![expression mStringValue]];
  end if;
  $end$; $write$; 
  returnedValue := valueWithVoid[];   
  returnedType := [@Ttype unconstructedType];   
end rule;

#---------------------------------------------------------------------------*

rule <template_instruction>
  ??@string inPrefix
  ??@string inPath
  ??@string inTemplateDirectory
  ?!@TfieldMap ioTemplateVariableMap
  ?!@string ioResultingString
  !@Tvalue returnedValue
  !@Ttype returnedType
:
  @string nextPrefix := inPrefix;
  $template$;
  select
    $if$; $exists$;
    $identifier$ ? @lstring templateName;
    @bool found;
    select
      $in$; $identifier$ ?@lstring prefix;
      nextPrefix := fullPrefix[!ioTemplateVariableMap !prefix];
    or
    end select;
    templateInvocation
      !templateName
      !nextPrefix
      !inPath
      !inTemplateDirectory
      !false
      !ioTemplateVariableMap
      !?ioResultingString
      ?found
    ;
    select
      $or$;
      #log found;
      parse when not found:
        @TfieldMap templateVariableMap [mapWithMapToOverride !ioTemplateVariableMap];
        <template_instruction_list>
          !nextPrefix
          !inPath
          !inTemplateDirectory
          !?templateVariableMap
          !?ioResultingString
          ?*
          ?*
        ;
        ioTemplateVariableMap := [templateVariableMap overriddenMap];
      else
        <template_instruction_list> parse;
      end parse;
      $end$; $template$;
    or
    end select;
  or
    $identifier$ ? @lstring templateName;
    select
      $in$; $identifier$ ?@lstring prefix;
      nextPrefix := fullPrefix[!ioTemplateVariableMap !prefix];
    or
    end select;
    templateInvocation
      !templateName
      !nextPrefix
      !inPath
      !inTemplateDirectory
      !true
      !ioTemplateVariableMap
      !?ioResultingString
      ?*
    ;
  end select;
  returnedValue := valueWithVoid[];   
  returnedType := [@Ttype unconstructedType];   
end rule;

#---------------------------------------------------------------------------*

rule <template_instruction>
  ??@string unused inPrefix
  ??@string unused inPath
  ??@string unused inTemplateDirectory
  ?!@TfieldMap ioTemplateVariableMap
  ?!@string ioResultingString
  !@Tvalue returnedValue
  !@Ttype returnedType
:
  $?$;
  $identifier$ ? @lstring columnConstantName;
  @string value := "";
  @bool searchEndOfLine := true;
  @uint idx := [ioResultingString length];
  loop [ioResultingString length] :
  while (idx > 0) & searchEndOfLine
  do
    searchEndOfLine := [ioResultingString characterAtIndex !idx - 1] != '\n';
    idx --;
    if searchEndOfLine then
      value .= " ";
    end if;
  end loop;
  [!?ioTemplateVariableMap insertKey
    !columnConstantName
    ![@Ttype stringType]
    !valueWithString[!value]
  ];
  returnedValue := valueWithVoid[];   
  returnedType := [@Ttype unconstructedType];   
end rule;

#---------------------------------------------------------------------------*

rule <template_instruction>
  ??@string unused inPrefix
  ??@string unused inPath
  ??@string unused inTemplateDirectory
  ?!@TfieldMap ioTemplateVariableMap
  ?!@string unused ioResultingString
  !@Tvalue returnedValue
  !@Ttype returnedType
:
  $function$;  
  $identifier$ ?@lstring fctName;
  @lstringlist args [emptyList];
  $($;
  select
    @lstring arg;
    $identifier$ ?arg;
    args += !arg;
    repeat while $,$; $identifier$ ?arg; args += !arg; end repeat;
  or end select;
  $)$;
  $functionContent$ ?@lstring content;
  content := [@lstring new !"%".[content string]."%" ![content location]];
  @Tfunction fct := [@Tfunction new !fctName !args ![content string]];
  [!?ioTemplateVariableMap insertKey
    !fctName
    ![@Ttype functionType]
    ![@Tvalue new !0LS !0.0 !"" ![@TfieldMapList emptyList] ![@TfieldMap emptyMap] !fct]
  ];
  returnedValue := valueWithVoid[];   
  returnedType := [@Ttype unconstructedType];   
end rule;

#---------------------------------------------------------------------------*

rule <template_instruction>
  ??@string inPrefix
  ??@string inPath
  ??@string inTemplateDirectory
  ?!@TfieldMap ioTemplateVariableMap
  ?!@string unused ioResultingString
  !@Tvalue returnedValue
  !@Ttype returnedType
:
  $call$;  
  $identifier$ ?@lstring fctName;
  @TexpressionList args [emptyList];
  $($;
  select
    @Ttype type;
    @Tvalue value;
    <expression>
      !inPrefix
      !inPath
      !inTemplateDirectory
      !ioTemplateVariableMap
      ?value
      ?type;
    args += !value !type;
    repeat while
      $,$;
      @Ttype type;
      @Tvalue value;
      <expression>
        !inPrefix
        !inPath
        !inTemplateDirectory
        !ioTemplateVariableMap
        ?value
        ?type;
      args += !value !type;
    end repeat;
  or
  end select;
  $)$;

  @Ttype type;
  @Tvalue value;
  [ioTemplateVariableMap searchKey
    !fctName
    ?type
    ?value
  ];
  
  if [args length] != [[[value mFunctionValue] args] length] then
    error here : "incorrect number of arguments";
  else
    # set up a variable map for local variables of the function
    @TfieldMap locals [emptyMap];
    # search arguments values and add them to the locals
    foreach args prefixedby actual_ , [[value mFunctionValue] args] prefixedby expected_ do
      [!?locals insertKey !expected_mValue !actual_mType !actual_mValue];
    end foreach;
    @string result := "";
#    message "calling ".[[value mFunctionValue] name]."\n";
    grammar template_grammar on [[value mFunctionValue] content] !inPrefix !inPath !inTemplateDirectory !locals !?result ?* ?*;
  end if;  
  returnedValue := valueWithVoid[];   
  returnedType := [@Ttype unconstructedType];   
end rule;

#---------------------------------------------------------------------------*

rule <template_instruction>
  ??@string inPrefix
  ??@string inPath
  ??@string inTemplateDirectory
  ?!@TfieldMap ioTemplateVariableMap
  ?!@string unused ioResultingString
  !@Tvalue returnedValue
  !@Ttype returnedType
:
  $return$;
  <expression> 
    !inPrefix
    !inPath
    !inTemplateDirectory
    !ioTemplateVariableMap
    ?returnedValue
    ?returnedType;
end rule;
