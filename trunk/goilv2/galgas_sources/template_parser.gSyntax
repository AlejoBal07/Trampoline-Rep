routine printVariable
  ?@uint indent
  ??@Ttype variableType
  ??@Tvalue variableValue
:
  indent := indent + 4;
  @string offset := [@string stringWithSequenceOfCharacters !' ' !indent];
  switch variableType
  when boolType:
    if [variableValue mInt64Value] == 1LS then
      message offset."true\n";
    else
      message offset."false\n";
    end if;
  when intType:
    message offset.[variableValue mInt64Value]."\n";
  when floatType:
    message offset.[variableValue mFloatValue]."\n";
  when stringType:
    message offset."\"".[variableValue mStringValue]."\"\n";
  when enumType:
    message offset.[variableValue mStringValue]."\n";
  when listType:
    foreach [variableValue mMapListValue] index idx do
      message offset."item at ".idx.":\n";
      printVariable !indent ![@Ttype structType] ![@Tvalue new !0LS !0.0 !"" ![@TfieldMapList emptyList] !mMap !dummyFunction[]];
    end foreach;
  when mapType:
    foreach [variableValue mStructValue] do
      message offset.lkey.":\n";
      printVariable !indent !mType !mValue;
    end foreach;
  when structType:
    foreach [variableValue mStructValue] do
      message offset.lkey.":\n";
      printVariable !indent !mType !mValue;
    end foreach;
  when unconstructedType, functionType:
  end switch;
end routine;

rule <template_instruction>
  ??@string inPrefix
  ??@string inPath
  ??@string inTemplateDirectory
  ?!@TfieldMap ioTemplateVariableMap
  ?!@string unused ioResultingString
  !@Tvalue returnedValue
  !@Ttype returnedType
:
  @TvarPath path;
  $display$; <variable> !inPrefix !inPath !inTemplateDirectory !ioTemplateVariableMap ?path;
  @Ttype variableType;
  @Tvalue variableValue;
  @location loc;
  searchField !path !ioTemplateVariableMap ?variableType ?variableValue ?loc;
  printPath !path;
  message " (".[loc locationString].") : ".[variableType messageGoilTemplateType]."\n";
  printVariable !0 !variableType !variableValue;
  returnedValue := valueWithVoid[];   
  returnedType := [@Ttype unconstructedType];   
end rule;

