/*
 * This configuration file adds a SOURCE attribute to ISR and COUNTER
 * It also define a new object type: INTERRUPT
 */
IMPLEMENTATION arm_interrupt {
  ISR [] {
    INTERRUPT_TYPE SOURCE;
  };
  COUNTER [] {
    INTERRUPT_TYPE SOURCE;
  };
  INTERRUPT [] { 
    UINT32 ID;
  };
  INTERRUPT_COUNT {
    UINT32 IT_TABLE_SIZE;
  };
};
