#ifndef TPL_SERVICES_IDS_H
#define TPL_SERVICES_IDS_H

#include "tpl_config_def.h"

/*=============================================================================
 * service ids
 */
%
let system_call_count := 0
foreach api_set in APICONFIG do
  foreach api in api_set::ENTRY do
    let system_call_count := system_call_count + 1
%
/**
 * @def OSServiceId_% !api::API %
 *
 * @see #SERVICE_CALL_DESCRIPTOR
 * @see #% !api::API %
 */
#define OSServiceId_% !api::API %    % !INDEX %
%
  end foreach
end foreach
%
/**
 * @def SYSCALL_COUNT
 *
 * Number of system calls
 */
#define SYSCALL_COUNT    % !system_call_count %

#endif

/* End of file TPL_SERVICES_IDS_H */

